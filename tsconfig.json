{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": false,
    "strict": true,
    "noImplicitAny": true,
    "useUnknownInCatchVariables": false,
    "inlineSourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "2022",
    "utils",
    "./.eslintrc.cjs",
  ],
  "exclude": [
    "node_modules",
  ]
}
